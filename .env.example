# SEC EDGAR API Configuration
# Format your User-Agent in the format: CompanyName/Version ContactEmail
SEC_EDGAR_USER_AGENT="AltmanZScore/1.0 your.email@domain.com"

# Optional: Legacy support (use SEC_EDGAR_USER_AGENT instead)
# SEC_API_EMAIL="your.email@domain.com"

# Azure OpenAI API Configuration (required for LLM features)
AZURE_OPENAI_API_KEY="your-azure-openai-api-key"
AZURE_OPENAI_ENDPOINT="your-azure-openai-endpoint"
AZURE_OPENAI_DEPLOYMENT="your-azure-openai-deployment"
AZURE_OPENAI_API_VERSION="2023-05-15"
AZURE_OPENAI_MODEL="gpt-35-turbo"

# Optional: Enable AI field mapping (default: false)
AI_FIELD_MAPPING="false"

# Optional: Data caching configuration
FINANCIAL_CACHE_TTL_DAYS=30
CACHE_DIR=".cache"

# Finnhub API Key
FINNHUB_API_KEY="your-finnhub-api-key"
